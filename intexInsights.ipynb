   "source": [
    "import pandas as pd \n",
    "\n",
    "df = pd.read_csv('Simplified_PIT_data.csv')\n",
    "\n",
    "df.head()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Group by Year and sum both sheltered and unsheltered homeless columns\n",
    "sheltered_by_year = df.groupby('Year')['Sheltered Total Homeless'].sum().reset_index()\n",
    "unsheltered_by_year = df.groupby('Year')['Unsheltered Homeless'].sum().reset_index()\n",
    "\n",
    "# Combine sheltered and unsheltered data\n",
    "total_homeless_by_year = pd.DataFrame({\n",
    "    'Year': sheltered_by_year['Year'],\n",
    "    'Sheltered Total': sheltered_by_year['Sheltered Total Homeless'],\n",
    "    'Unsheltered Total': unsheltered_by_year['Unsheltered Homeless'],\n",
    "    'Total Homeless': sheltered_by_year['Sheltered Total Homeless'] + unsheltered_by_year['Unsheltered Homeless']\n",
    "})\n",
    "\n",
    "# Select numeric columns, excluding 'Year'\n",
    "numeric_columns = df.select_dtypes(include=['float64', 'int64']).columns\n",
    "numeric_columns = numeric_columns.drop('Year')  # Drop 'Year' from numeric columns\n",
    "\n",
    "# Group by Year and sum all remaining numeric columns\n",
    "totals_by_year = df.groupby('Year')[numeric_columns].sum().reset_index()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
