<!-- Example Table of Volunteers -->
<h2><%= title %></h2>

<!-- Debug info -->
<% if (typeof error !== 'undefined' && error) { %>
  <div class="error-message">
    <%= error %>
  </div>
<% } %>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Sewing Level</th>
      <th>Hours Willing per Month</th>
    </tr>
  </thead>
  <tbody>
    <% if (typeof volunteers !== 'undefined' && volunteers && volunteers.length > 0) { %>
      <% volunteers.forEach(function(volunteer) { %>
        <tr>
          <td><%= volunteer.first_name %> <%= volunteer.last_name %></td>
          <td><%= volunteer.email %></td>
          <td><%= volunteer.level_description || 'Not Set' %></td>
          <td><%= volunteer.hours_willing || 'Not Set' %></td>
        </tr>
      <% }); %>
    <% } else { %>
      <tr>
        <td colspan="4">
          No volunteers found 
          <% if (typeof volunteers === 'undefined') { %>
            (Debug: volunteers is undefined)
          <% } else if (!volunteers) { %>
            (Debug: volunteers is null)
          <% } else { %>
            (Debug: empty array)
          <% } %>
        </td>
      </tr>
    <% } %>
  </tbody>
</table>